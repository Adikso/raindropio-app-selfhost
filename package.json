		"babel-plugin-lodash": "3.3.x",
		"clean-webpack-plugin": "^3.0.0",
		"css-loader": "3.5.x",
		"eslint": "6.x",
		"eslint-plugin-react": "7.x",
		"extract-text-webpack-plugin": "3.0.x",
		"file-loader": "6.0.x",
		"html-webpack-plugin": "4.2.x",
