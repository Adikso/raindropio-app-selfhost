		"link-module-alias": "^1.2.0",
		"lodash-webpack-plugin": "^0.11.5",
		"mini-css-extract-plugin": "^0.9.0",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
		"style-loader": "1.1.x",
		"stylus": "0.54.x",
