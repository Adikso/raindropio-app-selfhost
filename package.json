{
	"name": "app",
	"version": "5.0.0",
	"description": "",
	"author": "",
	"license": "ISC",
	"main": "index.js",
	"engines": {
		"node": "12.x"
	},
	"sideEffects": false,
	"scripts": {
		"local": "export NODE_ENV=development && export APP_PUBLIC_PATH=/ && webpack-dev-server --config build/config.js --no-clipboard --port 2000",
		"local:prod": "yarn build && serve dist/default -l 80",
		"build": "export NODE_ENV=production  && export APP_PUBLIC_PATH=/ && webpack --config build/config.js",
		"build:extension": "export NODE_ENV=production  && APP_TARGET='clipper' && webpack --config build/config.js",
		"build:desktop": "export NODE_ENV=production  && APP_TARGET='desktop' && webpack --config build/config.js",
		"size": "export NODE_ENV=production && export APP_PUBLIC_PATH=/ && webpack --config build/config.js --profile --json > dist/stats.json && webpack-bundle-analyzer dist/stats.json dist/default",
		"deploy:prod": "git branch -f release/production master && git push --force origin release/production",
		"preinstall": "command -v link-module-alias && link-module-alias clean || true",
		"postinstall": "link-module-alias"
	},
	"_moduleAliases": {
		"~network": "src/modules/network.js",
		"~config": "src/config.js",
		"~icon": "src/co/common/icon.js",
		"~t": "src/modules/translate/index.js",
		"~assets": "src/assets",
		"~co": "src/co",
		"~data": "src/data",
		"~local": "src/local",
		"~modules": "src/modules",
		"~routes": "src/routes",
		"~package.json": "package.json"
	},
	"dependencies": {
		"@sentry/browser": "5.x",
		"date-fns": "2.14.x",
		"ismobilejs": "^1.1.1",
		"localforage": "1.7.x",
		"lodash-es": "4.17.x",
		"masonic": "^3.2.0",
		"modern-normalize": "^0.6.0",
		"normalize-url": "4.x",
		"react": "16.13.x",
		"react-autosize-textarea": "7.0.x",
		"react-beautiful-dnd": "^13.0.0",
		"react-dom": "16.13.x",
		"react-helmet": "6.1.x",
		"react-portal": "^4.2.1",
		"react-router-dom": "5.2.x",
		"react-sortablejs": "^2.0.11",
		"react-textarea-autosize": "^8.0.1",
		"react-virtuoso": "0.17.x",
		"string": "3.3.x",
		"validator": "13.1.x"
	},
	"optionalDependencies": {
		"@sentry/minimal": "5.x",
		"react-redux": "7.2.x",
		"redux": "4.x",
		"redux-persist": "6.0.x",
		"redux-saga": "1.1.x",
		"reselect": "4.x",
		"seamless-immutable": "7.x"
	},
	"devDependencies": {
		"@babel/core": "7.x",
		"@babel/plugin-proposal-class-properties": "7.x",
		"@babel/plugin-proposal-decorators": "7.x",
		"@babel/plugin-proposal-object-rest-spread": "7.x",
		"@babel/plugin-syntax-dynamic-import": "7.x",
		"@babel/preset-env": "7.x",
		"@babel/preset-react": "7.x",
		"babel-eslint": "10.x",
		"babel-loader": "8.x",
		"babel-plugin-lodash": "3.3.x",
		"clean-webpack-plugin": "^3.0.0",
		"css-loader": "3.6.x",
		"eslint": "7.x",
		"eslint-plugin-react": "7.x",
		"extract-text-webpack-plugin": "3.0.x",
		"file-loader": "6.0.x",
		"html-webpack-plugin": "4.3.x",
		"image-webpack-loader": "6.0.x",
		"link-module-alias": "^1.2.0",
		"lodash-webpack-plugin": "^0.11.5",
		"mini-css-extract-plugin": "^0.9.0",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
		"prerender-loader": "^1.3.0",
		"serve": "^11.3.0",
		"style-loader": "1.2.x",
		"stylus": "0.54.x",
		"stylus-loader": "3.0.x",
		"svg-sprite-loader": "5.0.x",
		"terser-webpack-plugin": "3.0.x",
		"webpack": "4.43.x",
		"webpack-bundle-analyzer": "3.8.x",
		"webpack-cli": "^3.3.11",
		"webpack-dev-server": "3.11.x",
		"write-file-webpack-plugin": "4.5.x"
	}
}
