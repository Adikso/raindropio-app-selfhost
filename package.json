  "scripts": {
    "startDev": "webpack-dev-server --config internals/webpack.dev.js --content-base src/ --public-path build/ --progress --colors --port 80 --hot --inline --no-info --display-error-details",
    "build": "npx webpack --production --config internals/webpack.web.build.js --profile --colors",
    "dev": "export RAIN_PLATFORM=dev && npm run startDev",
    "desktop": "rm -rf ../Desktop/app/app && webpack --production --config internals/webpack.desktop.build.js && cd ../Desktop && npm run dist && cd ../App",
    "clipper": "rm -r ../Clipper/appbuild && webpack --production --config internals/webpack.clipper.build.js",
    "sourcepack": "zip -FSr -q -X source.zip clipper internals src package.json versionfix.js README.md",
    "deploy:prod": "git branch -f release/production master && git push --force origin release/production"
  },
