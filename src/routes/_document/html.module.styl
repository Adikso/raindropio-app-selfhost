figure {
    margin: 0
}

progress::-webkit-progress-value {
    transition: width .3s ease-in-out;
}

html, body, :global(#react) {
    overflow: hidden
    margin:0
    width: 100%
    height: 100%
}

html:global(.extension):global(.browser_action) {
    width: auto
    height: auto

    body, :global(#react) {
        width: auto
        height: auto
    }
}

[hidden] {
    display: none !important;
}