{
    "presets": ["@babel/preset-react"],
    "plugins": [
        ["@babel/plugin-proposal-class-properties",{"useBuiltIns": false}]
    ],
    "env": {
        "production": {
            "presets": [
                [
                    "@babel/env",
                    {
                        "targets": {
                            "esmodules": true
                        },
                        "modules": false,
                        "useBuiltIns": "usage",
                        "corejs": 3
                    }
                ]
            ],
            "plugins": [
                ["@babel/plugin-proposal-object-rest-spread",{"useBuiltIns": false}],
                "@babel/plugin-syntax-dynamic-import",
                "lodash"
            ]
        },

        "development": {
        }
    }
}