{
    "presets": ["@babel/preset-react"],
    "plugins": [
        ["@babel/plugin-proposal-decorators",{"legacy": true}],
        ["@babel/plugin-proposal-class-properties",{"useBuiltIns": true, "corejs": 3}],
        ["@babel/plugin-proposal-object-rest-spread",{"useBuiltIns": true, "corejs": 3}],
        "@babel/plugin-syntax-dynamic-import"
    ],

    "env": {
        "production": {
            "presets": [
                [
                    "@babel/env",
                    {
                        "useBuiltIns": "usage",
                        "corejs": 3,
                        "targets": {
                            "esmodules": true
                        }
                    }
                ]
            ],
            "plugins": [
                "lodash"
            ]
        },

        "development": {
            
        }
    }
}